{
 "extends": "astro/tsconfigs/base",
 "compilerOptions": {
  "baseUrl": ".",
  "strictNullChecks": true,
  "allowJs": true,
  "paths": {
   "@assets/*": ["./src/assets/*"],
   "@layouts/*": ["./src/layouts/*"],
   "@components/*": ["./src/components/*"],
   "@content/*": ["./src/content/*"],
   "@styles/*": ["./src/styles/*"],
   "@lib/*": ["./src/lib/*"],
   "@data/*": ["./src/data/*"]
  },
  "jsx": "react-jsx",
  "jsxImportSource": "react"
 }
}
